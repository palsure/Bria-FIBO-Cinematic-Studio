# LLM API Keys (choose one)
OPENAI_API_KEY=your_openai_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here

# BRIA AI API Configuration
# Get your API token from: https://platform.bria.ai (Organization Settings > API Keys)
BRIA_API_TOKEN=your_bria_api_token_here

# BRIA API Base URL (from your dashboard)
# Production: https://engine.prod.bria-api.com/v2 or /v1
BRIA_API_BASE_URL=https://engine.prod.bria-api.com/v2

# BRIA Model ID for tailored generation (optional, check dashboard for available models)
# BRIA_MODEL_ID=your_model_id_here

# Output Settings
OUTPUT_DIR=./outputs
HDR_ENABLED=true
BIT_DEPTH=16

# Optional: Local LLM
LOCAL_LLM_PATH=./models/llm
LOCAL_LLM_ENABLED=false

